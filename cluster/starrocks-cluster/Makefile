include ../../.env
include .env

CONTAINERS=

.PHONY: help test
help:
	@echo "usage: make <option>"
	@echo "options and effects:"
	@echo "    help                     : Show help"
	@echo "    test                     : Test ..."
	@echo "    build      				: build"
	@echo "    start                    : 启动所有服务"

test:
	@echo "test ..."

.PHONY: build start
build:
	@${DC_BUILD} ${CONTAINERS}

start:
	@${DC_UP} ${CONTAINERS}
