###########################################################
###################### General Setup ######################
###########################################################

### Drivers ################################################

# All volumes driver
VOLUMES_DRIVER=local

# TimeZone ################################################
TZ=Asia/Shanghai

# Choose storage path on your machine. For all storage systems
DATA_PATH_HOST=~/.docker-env/nacos-local

# log storage path on your machine.
LOG_PATH_HOST=../logs/nacos-local

# Define the prefix of container names. This is useful if you have multiple projects that use laradock to have separate containers per project.
COMPOSE_PROJECT_NAME=docker-env-nacos-local

### COMMON #####################################################

CONF_HOST_PATH=./conf

### NACOS ########################################

NACOS_CONTAINER_NAME=nacos
NACOS_HOSTNAME=nacos
NACOS_VERSION=v2.3.0
NACOS_HOST_PATH=./nacos
NACOS_FRONTEND_IP=172.10.3.201
NACOS_BACKEND_IP=172.11.3.201